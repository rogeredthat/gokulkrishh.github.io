{
  "name": "gokulkrishh.github.io",
  "version": "1.0.0",
  "description": "Source code for my blog (Jekyll 3.0)",
  "main": "\"\"",
  "scripts": {
    "drafts": "npm run concat-files && jekyll server --host=0.0.0.0 --drafts --watch",
    "server": "npm run concat-files && jekyll server --host=0.0.0.0 --watch",
    "concat-files": "concat -o ./css/styles.css ./css/base.css ./css/about.css ./css/post.css ./css/syntax.css ./css/screens.css ./css/examples.css",
    "precache": "./node_modules/sw-precache/cli.js --config=sw-precache-config.json",
    "build": "npm run concat-files && npm run precache && jekyll build"
  },
  "devDependencies": {
    "concat": "^1.0.3",
    "sw-cli": "^0.0.18",
    "sw-precache": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gokulkrishh/gokulkrishh.github.io.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gokulkrishh/gokulkrishh.github.io/issues"
  },
  "homepage": "https://github.com/gokulkrishh/gokulkrishh.github.io#readme"
}
